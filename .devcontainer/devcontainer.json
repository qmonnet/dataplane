{
	image: "dpdk-sysroot",
	"name": "dpdk-dev-container",
	"mounts": [
		"source=/mnt/huge/2M,target=/mnt/huge/2M,type=bind",
		"source=/mnt/huge/1G,target=/mnt/huge/1G,type=bind"
	],
	"privileged": true,
	"containerEnv": {
		"DPDK_INSTALL_PATH": "/",
		"LIBRARY_PATH": "/usr/lib"
	},
	"runArgs": [
		"--network=host"
	]
}
